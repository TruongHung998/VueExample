   },\n        {\n          name: \"Tải App\",\n          slug: '/download-app'\n        }\n      ]\n    }\n  },\n\n  mounted() {\n    window.addEventListener('scroll', () => {\n      const scrollPos = window.scrollY\n      if (scrollPos >= 200) {\n        this.isSticky = true\n      } else {\n        this.isSticky = false\n      }\n    })\n  }\n}\n\n</script>\n\n<style lang=\"scss\">\n.header {\n  &.fixed-top {\n    z-index: 99;\n  }\n}\n</style>\n\n<style lang=\"scss\" scoped>\n@import \"header\";\n</style>\n"],"sourceRoot":""}]}