pastBottomOffset : pastTopOffset\n      this.scrollFn(this)\n    },\n    /**\n     * The function who make the magics\n     * @return {void}\n     */\n    backToTop () {\n      window.smoothscroll()\n      this.$emit('scrolled')\n    }\n  },\n}\n</script>\n<style src=\"./styles.css\"></style>\n"],"sourceRoot":""}]}